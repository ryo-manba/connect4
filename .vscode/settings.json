{
    "code-runner.executorMap": {
        "cpp": "cd $dir && rm $dir/$fileNameWithoutExt && clang++ -std=c++17 $fileName -o $fileNameWithoutExt && $dir/$fileNameWithoutExt",
    },
    "code-runner.clearPreviousOutput": true,
    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "lldb.launch.expressions": "native",
    "C_Cpp.updateChannel": "Insiders",
    "git.autofetch": true,
    "explorer.confirmDelete": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "explorer.confirmDragAndDrop": false,
    "workbench.startupEditor": "welcomePage",
    "editor.renderWhitespace": "all",
    "debug.onTaskErrors": "abort",
    "tabnine.experimentalAutoImports": true,
    "editor.snippetSuggestions": "top",

    // 42setting
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "42header.email": "rmatsuka@student.42tokyo.jp",
    "42header.username": "rmatsuka",
    "code-runner.languageIdToFileExtensionMap": {

        "bat": ".bat",
        "powershell": ".ps1",
        "typescript": ".ts",
        "c": "cd $dir && gcc -Wall -Wextra -Werror $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    },
    "files.associations": {
        "string.h": "c",
        "array": "c",
        "string": "c",
        "string_view": "c",
        "ranges": "c",
        "libft.h": "c",
        "tester.h": "c",
        "push_swap.h": "c"
    },
}

